        }
    ],
    "license": "Propierty",
    "repositories": [{
        "type": "composer",
        "url": "https://packagist.wach.id"
    }],
    "require": {
        "php": "^8.2",
        "turahe/master-data": "^1.0",
        "turahe/core": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
